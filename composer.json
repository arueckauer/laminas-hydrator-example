{
    "name": "rueckauer/laminas-hydrator-example",
    "description": "Example project for the Laminas Hydrator",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Andi RÃ¼ckauer",
            "email": "arueckauer@gmail.com"
        }
    ],
    "require": {
        "php": "~8.2.0",
        "laminas/laminas-config-aggregator": "^1.15",
        "laminas/laminas-hydrator": "^4.15",
        "laminas/laminas-servicemanager": "^3.22"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "~2.44.0",
        "laminas/laminas-coding-standard": "~3.0.1",
        "phpunit/phpunit": "~10.5.36",
        "rector/rector": "~1.2.7",
        "vimeo/psalm": "~5.26.1"
    },
    "autoload": {
        "psr-4": {
            "LaminasHydratorExample\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasHydratorExampleTest\\": "test/unit/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
